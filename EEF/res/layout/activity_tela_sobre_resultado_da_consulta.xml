<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".TelaSobreResultadoDaConsulta" >

    <TextView
        android:id="@+id/sobre_resultado_da_consulta_title"
        style="@style/tituloSobre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/title_sobre_resultado_da_consulta"
        android:textAppearance="?android:attr/textAppearanceLarge" />
    
      <TextView
        android:id="@+id/conteudo_sobre_resultado_da_consulta"
        style="@style/subTituloSobre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/conteudo_sobre_resultado_da_consulta"
        android:layout_below="@+id/conteudo_sobre_resultado_da_consulta"
        android:layout_marginTop="34dp"
        android:text="@string/conteudo_sobre_resultado_da_consulta"
        android:textAppearance="?android:attr/textAppearanceMedium" />

</RelativeLayout>