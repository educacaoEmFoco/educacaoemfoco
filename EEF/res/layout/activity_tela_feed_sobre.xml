<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".FeedAboutScreen" 
    android:background="@drawable/background">

    <TextView
        android:id="@+id/textView_sobre_feed"
        style="@style/tituloSobre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="26dp"
        android:text="@string/tela_sobre_feed" />

     <TextView
         android:id="@+id/textView_sobre_feed_texto"
         style="@style/subTituloSobre"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_alignLeft="@+id/textView_sobre_feed"
         android:layout_below="@+id/textView_sobre_feed"
         android:layout_marginTop="15dp"
         android:text="@string/descricao_sobre_feed" />

</RelativeLayout>
